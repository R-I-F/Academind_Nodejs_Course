<%- include('../includes/head.ejs') %>
</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <% if (orders.length > 0) { %>
            <% orders.forEach(order => { %>
                <ul>
                    <% order.products.forEach(prod => { %>
                        <li><%= `${prod.title} (${prod.orderItem.qty})`%></li>
                    <% }) %>
                </ul>
            <% }) %>
        <% } else { %>
            <p>No orders found.</p>
        <% } %>
    </main>
    <%- include('../includes/end.ejs') %>
</body>
